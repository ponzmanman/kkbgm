{
  "name": "KKBGM",
  "description": "Extension for Yahoo News with related track",
  "version": "1.0",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "storage"
  ],
  "content_scripts": [{
    "matches": [
      "https://tw.news.yahoo.com/*"
    ],
    "js": [
      "jquery-1.12.4.min.js",
      "content.js"
    ]
  }],
  "browser_action": {
    "default_title": "Help KKBGM.",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "icons": {
    "16": "./icon.png",
    "48": "./icon_32.png",
    "128": "./icon_128.png"
  }
}